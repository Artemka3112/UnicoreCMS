{
  "name": "unicore-cms",
  "private": true,
  "workspaces": [
    "workspaces/*"
  ],
  "scripts": {
    "dev": "npm run build --workspace=unicore-common && lerna run dev --parallel --no-bail",
    "build": "npm run build -w unicore-common && npm run build --workspaces --if-present",
    "start": "lerna run start --parallel --no-bail",
    "lint": "npm run lint --workspaces --if-present",
    "format": "npm run format --workspaces --if-present",
    "sync": "npm run schema:sync -w unicore-server",
    "seed": "npm run seed:run -w unicore-server"
  },
  "devDependencies": {
    "lerna": "^4.0.0",
    "typescript": "^4.5.5"
  }
}
